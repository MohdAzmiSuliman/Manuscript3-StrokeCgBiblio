@article{ WOS:000779537800001,
Author = {LeLaurin, Jennifer H. and Sypniewski, Caroline and Wing, Kristen and
   Freytes, I. Magaly and Findley, Kimberly and Uphold, Constance R.},
Title = {Development, Usability Testing, and Promotion of the English- and
   Spanish-language RESCUE Stroke Caregiver Websites},
