   William E. and Wadley, Virginia G. and Perkins, Martinique M. and Roth,
   David L.},
Editor = {Wu, J and Yang, J and Nakagoshi, N and Lu, X and Xu, H},
Title = {Does Caregiver Well-Being Predict Stroke Survivor Depressive Symptoms? A
   Mediation Analysis},
Booktitle = {NATURAL RESOURCES AND SUSTAINABLE DEVELOPMENT II, PTS 1-4},
