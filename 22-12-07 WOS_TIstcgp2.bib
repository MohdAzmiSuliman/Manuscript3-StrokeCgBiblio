@article{ WOS:000881776500009,
Author = {Rehman, Awais Ur and Niazi, Rauf and Rehman, Hassan Ur and Javed, Ayesha},
Title = {Assessment of quality of life of stroke survivors and their caregivers
   presenting to a tertiary care hospital in Pakistan},
