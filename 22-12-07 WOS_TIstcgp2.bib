@article{ WOS:000865816900014,
Author = {King, Rosemarie B. and Raad, Jason H. and Flaherty, Jennifer and Hartke,
   Robert J.},
Title = {Stroke Caregiver Depression Qualitative Comparison of Treatment
   Responders and Nonresponders at 1 Year},
Journal = {JOURNAL OF CARDIOVASCULAR NURSING},
