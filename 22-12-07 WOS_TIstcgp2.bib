@article{ WOS:000668415000018,
Author = {Liu, Zhe and King, Malizia},
Title = {The influencing factors and coping styles of mental health stress
   responses of stroke caregivers},
Journal = {WORK-A JOURNAL OF PREVENTION ASSESSMENT \& REHABILITATION},
